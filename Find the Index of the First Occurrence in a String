class Solution {
public:
    int strStr(string haystack, string needle) {
        int n=haystack.size();
        int m=needle.size();
        int ind=-1;
        int j=0; 
        for(int i=0; i<n-m+1 ; i++){
            if(haystack.substr(i , m)==needle){
                 
                return i;
            } 
             
        }
        return -1;
    }
};
